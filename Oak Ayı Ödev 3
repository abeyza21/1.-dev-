for i in range(1,100):
  print(i)

for i in range(1,100):
  if i%2==0:
    print(i)

import math
sayi=int(input("Faktoriyeli hesaplanacak say覺y覺 giriniz "))
sonuc=math.factorial(sayi)
print(f" Girilen say覺n覺n faktoriyeli :{ sonuc}")

from sympy import primerange
asal_sayilar=list(primerange(1,100))
print (asal_sayilar)
